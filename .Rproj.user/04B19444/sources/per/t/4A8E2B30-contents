frequency<-function(sequence_in, k) {

if(k==2){
dinucleotide_freq <- kmer_frequency(sequence_in, 2)
print(dinucleotide_freq)
}
if(k==5){
  pentanucleotide_freq <- kmer_frequency(sequence_in, 5)
  print(pentanucleotide_freq)
}
  if(k==3)
  {
tri_freq<-kmer_frequency(sequence_in, 3)
print(tri_freq)
  }

}
